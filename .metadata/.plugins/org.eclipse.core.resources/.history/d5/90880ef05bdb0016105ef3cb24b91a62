public class Stadt extends Ortschaft
{
    public Stadt()
    {
    }

    @Override
    public Gebaeude upgrade()
    {

        final Ortschaft grossstadt = new OrtschaftFactory().create("grossstadt");
        grossstadt.setUpgradedauer(6);
        return grossstadt;
    }

    @Override
    public List<Gebaeude> getBenoetigteGebaude()
    {
        // TODO Auto-generated method stub
        return null;
    }

    @Override
    public RohstoffKosten getBaukosten()
    {
        // TODO Auto-generated method stub
        return null;
    }

}
