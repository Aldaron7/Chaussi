package chaussia.shared.rohstoffe;

public enum Rohstofftyp
{
    HOLZ(Rohstofftyp.menge), LEHM(0), KORN(0), ERZ, WOLLE, BRETT, ZIEGEL, BROT, EISEN, STOFF;

    private static int menge;

    private Rohstofftyp(int menge)
    {
        // TODO Auto-generated constructor stub
    }

    @Override
    public String toString()
    {
        return this.name().toLowerCase();
    }
}
