package chaussia.shared.gebaeude.ortschaft;

import java.util.ArrayList;
import java.util.List;

import chaussia.shared.gebaeude.RohstoffKosten;
import chaussia.shared.gebaeude.String;

public class Dorf extends Ortschaft
{
    private static final long serialVersionUID = 1L;

    public Dorf()
    {
    }

    @Override
    public String upgrade()
    {
        final Ortschaft stadt = new OrtschaftFactory().create("stadt");
        stadt.setUpgradedauer(3);
        return stadt;
    }

    @Override
    public List<String> getBenoetigteGebaude()
    {
        return new ArrayList<>();
    }

    @Override
    public RohstoffKosten getBaukosten()
    {
        // TODO Auto-generated method stub
        return null;
    }

}
