package chaussia.shared.gebaeude.ortschaft;

import java.util.List;

import chaussia.shared.gebaeude.AbstractGebaeude;
import chaussia.shared.gebaeude.RohstoffKosten;

public class Stadt extends AbstractOrtschaft
{
    private static final long serialVersionUID = 1L;

    public Stadt()
    {
    }

    @Override
    public AbstractGebaeude upgrade()
    {

        final AbstractOrtschaft grossstadt = new OrtschaftFactory().create(Typ.GROSSSTADT);
        grossstadt.setUpgradedauer(6);
        return grossstadt;
    }

    @Override
    public List<Typ> getBenoetigteGebaude()
    {
        // TODO Auto-generated method stub
        return null;
    }

    @Override
    public RohstoffKosten getBaukosten()
    {
        // TODO Auto-generated method stub
        return null;
    }

}
