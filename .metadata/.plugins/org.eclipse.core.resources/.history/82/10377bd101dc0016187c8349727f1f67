    {
        return Arbeiter.Typ.EINFACHER_ARBEITER;
    }

    @Override
    public List<Typ> getBenoetigteGebaude()
    {
        // TODO Auto-generated method stub
        return null;
    }

    @Override
    public RohstoffKosten getBaukosten()
    {
        // TODO Auto-generated method stub
        return null;
    }

}
