package chaussia.shared.gebaeude;

import java.util.List;

import chaussia.shared.gebaeude.Gebaeude;
import chaussia.shared.gebaeude.RohstoffKosten;

public class Dorf extends Ortschaft
{
    public Dorf()
    {
    }

    @Override
    public Gebaeude upgrade()
    {
        final Ortschaft stadt = new OrtschaftFactory().create("stadt");
        stadt.setUpgradedauer(3);
        return stadt;
    }

    @Override
    public List<Gebaeude> getBenoetigteGebaude()
    {
        // TODO Auto-generated method stub
        return null;
    }

    @Override
    public RohstoffKosten getBaukosten()
    {
        // TODO Auto-generated method stub
        return null;
    }

}
